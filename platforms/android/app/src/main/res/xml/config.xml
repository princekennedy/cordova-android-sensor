<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.psensors" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Sensors">
        <param name="android-package" value="com.fabiorogeriosj.plugin.Sensors" />
    </feature>
    <name>Android Ons Sense</name>
    <description>
        This App use all android sensors
    </description>
    <author email="prince0888407189@gmail.com" href="http://cordova.io">
        Prince Kennedy
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
